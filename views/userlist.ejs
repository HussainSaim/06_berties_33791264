<!DOCTYPE html>
<html>
  <head>
    <title>User List - <%=shopData.shopName%></title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>
  <body>
    <h1>Registered Users</h1>
    <table border="1">
      <tr>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>
      <% availableUsers.forEach(function(user) { %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.firstName %></td>
          <td><%= user.lastName %></td>
          <td><%= user.email %></td>
        </tr>
      <% }); %>
    </table>
    <p><a href="/">Back to Home</a></p>
  </body>
</html>
